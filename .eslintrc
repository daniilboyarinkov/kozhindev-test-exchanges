{
    "extends": ["plugin:import/recommended"],
    "plugins": ["jsx-a11y"],
    "rules": {
        "import/order": [
            1,
            {
                "groups": [
                    "external",
                    "builtin",
                    "index",
                    "sibling",
                    "parent",
                    "internal",
                    "type"
                ],
                "alphabetize": {
                    "order": "asc",
                    "caseInsensitive": true
                },
                "newlines-between": "always-and-inside-groups"
            }
        ],
        "import/no-unresolved": [1, { "caseSensitive": false }]
    },
    "settings": {
        "import/parsers": {
            "@typescript-eslint/parser": [".ts", ".tsx", "js", "jsx"]
        }
    }
}
